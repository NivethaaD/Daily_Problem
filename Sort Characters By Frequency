class Solution:
    def frequencySort(self, s: str) -> str:
        d = dict()
        ans = ""
        for ele in s:
            if d.get(ele,'') != '':
                d[ele]=d[ele]+1
            else:
                d[ele]=1
        
        d = dict(sorted(d.items(), key=lambda a: a[1], reverse=True))
        for ele in d:
            ans = ans + (ele*d[ele])

        return ans
