class Solution:
    def isAnagram(self, s: str, t: str) -> bool:

        if len(s)!= len(t):
            return False
        
        d1 = dict()
        
        for val in s:
            if val in d1:
                d1[val]=d1[val]+1
            else:
                d1[val]=1
        
        d2 = dict()
        
        for val in t:
            if val in d2:
                d2[val]=d2[val]+1
            else:
                d2[val]=1
            
        for ele in d1:
            if d1[ele] != d2.get(ele,''):
                return False
        
        return True
