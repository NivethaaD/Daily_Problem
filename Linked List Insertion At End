'''    
class Node:
    def __init__(self, data):
        self.data = data
        self.next = None
'''

class Solution:
    def insertAtEnd(self, head, x):
        tempNode = head
        if head == None:
            head = Node(x)
            return head

        if tempNode.next == None:
            return head

        actualNode = Node(x)
        while tempNode.next != None:
            tempNode = tempNode.next
        tempNode.next = actualNode
        return head
